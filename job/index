    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const searchInput = document.getElementById('search-input');
            const searchForm = document.getElementById('job-search-form');
            const jobItems = document.querySelectorAll('.job-listing .related-item');
            const noResultsMessageJS = document.querySelector('.job-listing > #no-search-results[style*="display: none"]');
            const noResultsStatic = document.querySelector('.job-listing > #no-search-results:not([style*="display: none"])');

            function performSearch(searchTerm) { /* ... search logic remains the same ... */
                let visibleCount = 0;
                const term = searchTerm.toLowerCase().trim();
                jobItems.forEach(item => {
                    const searchableText = item.getAttribute('data-searchable-text') || '';
                    if (!term || searchableText.includes(term)) {
                        item.classList.remove('hidden-by-search'); visibleCount++;
                    } else { item.classList.add('hidden-by-search'); }
                });
                if (noResultsStatic) { noResultsStatic.style.display = 'none'; }
                if (noResultsMessageJS) { noResultsMessageJS.style.display = (visibleCount === 0 && term !== '') ? 'block' : 'none'; }
            }
            if (searchInput && searchForm) {
                const urlParams = new URLSearchParams(window.location.search);
                const initialSearchTerm = urlParams.get('search');
                if (initialSearchTerm !== null) {
                    searchInput.value = initialSearchTerm;
                    if (jobItems.length > 0) { performSearch(initialSearchTerm); }
                    else { if (noResultsStatic) noResultsStatic.style.display = 'none'; if (noResultsMessageJS) noResultsMessageJS.style.display = 'block'; }
                } else { if (noResultsMessageJS) noResultsMessageJS.style.display = 'none'; }
             } else { /* console warnings */ }
        });
    </script>


